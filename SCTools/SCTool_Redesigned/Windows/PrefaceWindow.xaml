<Window x:Class="SCTool_Redesigned.Windows.PrefaceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SCTool_Redesigned.Windows"
        xmlns:lang="clr-namespace:SCTool_Redesigned.Properties"
        mc:Ignorable="d"
        Title="{x:Static lang:Resources.UI_Title_ProgramTitle}" Height="180" Width="360" Margin="2,0" Background="{DynamicResource BackgroundBrush}" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterScreen" WindowStyle="SingleBorderWindow">
    <Window.Effect>
        <DropShadowEffect/>
    </Window.Effect>
    <Grid Background="{DynamicResource BackgroundBrush}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

    </Grid>

</Window>
